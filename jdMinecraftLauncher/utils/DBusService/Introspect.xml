<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN"
"http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node>
  <interface name="page.codeberg.JakobDev.jdMinecraftLauncher">
    <method name="OpenURI">
      <arg direction="in" type="s" name="uri"/>
    </method>

    <method name="ListProfiles">
      <arg direction="out" type="as" name="profiles"/>
    </method>

    <method name="LaunchProfile">
      <arg direction="in" type="s" name="id"/>
    </method>

    <method name="GetProfileDetails">
      <arg direction="in" type="s" name="id"/>
      <arg direction="out" type="a{sv}" name="details"/>
    </method>

    <method name="GetLauncherVersion">
      <arg direction="out" type="s" name="version"/>
    </method>
  </interface>

  <interface name="org.kde.krunner1">
    <method name="Actions">
      <arg direction="out" type="a(sss)" name="matches"/>
    </method>

    <method name="Match">
      <arg direction="in" type="s" name="query"/>
      <arg direction="out" type="a(sssida{sv})" name="matches"/>
    </method>

    <method name="Run">
      <arg direction="in" type="s" name="matchId"/>
      <arg direction="in" type="s" name="actionId"/>
    </method>
  </interface>

  <interface name="org.gnome.Shell.SearchProvider2">

    <method name="GetInitialResultSet">
      <arg type="as" name="terms" direction="in" />
      <arg type="as" name="results" direction="out" />
    </method>

    <method name="GetSubsearchResultSet">
      <arg type="as" name="previous_results" direction="in" />
      <arg type="as" name="terms" direction="in" />
      <arg type="as" name="results" direction="out" />
    </method>

    <method name="GetResultMetas">
      <arg type="as" name="identifiers" direction="in" />
      <arg type="aa{sv}" name="metas" direction="out" />
    </method>

    <method name="ActivateResult">
      <arg type="s" name="identifier" direction="in" />
      <arg type="as" name="terms" direction="in" />
      <arg type="u" name="timestamp" direction="in" />
    </method>

    <method name="LaunchSearch">
      <arg type="as" name="terms" direction="in" />
      <arg type="u" name="timestamp" direction="in" />
    </method>
  </interface>

  <interface name="org.freedesktop.DBus.Introspectable">
    <method name="Introspect">
      <arg direction="out" type="s"/>
    </method>
  </interface>

  <interface name="org.freedesktop.DBus.Peer">
    <method name="Ping" />
    <method name="GetMachineId">
      <arg direction="out" type="s"/>
    </method>
  </interface>
</node>
